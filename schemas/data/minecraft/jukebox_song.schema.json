{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/Mosberg/minecraft-json-schema-workspace/refs/heads/main/schemas/data/minecraft/jukebox_song.schema.json",
  "title": "Minecraft Jukebox Song",
  "description": "Schema for jukebox song JSON files in Minecraft 1.21.11",
  "type": "object",
  "properties": {
    "comparator_output": { "type": "integer" },
    "description": {
      "type": "object",
      "properties": {
        "translate": { "type": "string" }
      },
      "required": ["translate"]
    },
    "length_in_seconds": { "type": "number" },
    "sound_event": { "type": "string" }
  },
  "required": ["comparator_output", "description", "length_in_seconds", "sound_event"]
}
